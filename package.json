{
  "name": "@lobehub/chat-plugins-gateway",
  "version": "1.9.0",
  "description": "Lobe Chat Plugin Market with Datafiniti and Tavily plugins",
  "repository": "https://github.com/lobehub/chat-plugins-gateway.git",
  "author": "LobeHub <i@lobehub.com>",
  "sideEffects": false,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "father build",
    "build:watch": "father dev",
    "ci": "npm run lint && npm run type-check && npm run doctor",
    "doctor": "father doctor",
    "generate-index": "ts-node src/scripts/generateIndex.ts",
    "lint": "eslint \"{src,api,docs}/**/*.{js,jsx,ts,tsx}\" --fix",
    "lint:md": "remark . --quiet --frail --output",
    "prepare": "husky install",
    "prepublishOnly": "npm run build",
    "prettier": "prettier -c --write \"**/**\"",
    "release": "semantic-release",
    "start": "vercel dev",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "test:update": "vitest -u",
    "type-check": "tsc --noEmit"
  },
  "lint-staged": {
    "*.md": [
      "remark --quiet --output --",
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.json": [
      "prettier --write --no-error-on-unmatched-pattern"
    ],
    "*.{js,jsx}": [
      "prettier --write",
      "eslint --fix"
    ],
    "*.{ts,tsx}": [
      "prettier --parser=typescript --write",
      "eslint --fix"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 10"
  ],
  "dependencies": {
    "@babel/runtime": "^7",
    "@cfworker/json-schema": "^1",
    "@lobehub/chat-plugin-sdk": "^1",
    "ajv": "^8",
    "antd": "^5",
    "antd-style": "^3.7.1",
    "lucide-react": "^0.503.0",
    "next": "^14.0.3",
    "react-layout-kit": "^1.9.1",
    "swagger-client": "^3",
    "zod": "^3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17",
    "@lobehub/lint": "latest",
    "@types/node": "^20.10.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vercel/node": "^2",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-v8": "0.34.6",
    "commitlint": "^17",
    "cross-env": "^7",
    "eslint": "^8",
    "father": "4.3.1",
    "husky": "^8",
    "lint-staged": "^13",
    "prettier": "^3",
    "remark": "^14",
    "remark-cli": "^11",
    "semantic-release": "^21",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "vercel": "^32.7.2",
    "vite": "^6.3.3",
    "vitest": "0.34.6"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  }
}
